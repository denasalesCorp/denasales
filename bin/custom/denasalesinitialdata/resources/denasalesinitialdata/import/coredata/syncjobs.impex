$sourceProductCV=sourceVersion(catalog(id[default=denasalesProductCatalog]),version[default='Staged'])[unique=true,default='denasalesProductCatalog:Staged']
$targetProductCV=targetVersion(catalog(id[default=denasalesProductCatalog]),version[default='Online'])[unique=true,default='denasalesProductCatalog:Online']
$sourceContentCV=sourceVersion(catalog(id[default=denasalesContentCatalog]),version[default='Staged'])[unique=true,default='denasalesContentCatalog:Staged']
$targetContentCV=targetVersion(catalog(id[default=denasalesContentCatalog]),version[default='Online'])[unique=true,default='denasalesContentCatalog:Online']
$contentCatalogsRootTypes=rootTypes(code)[default='CMSItem,CMSRelation']
 
#Insert Product Catalogs Synchronization
INSERT_UPDATE CatalogVersionSyncJob ; code[unique=true]                           ; $sourceProductCV ; $targetProductCV ;  
                                    ; sync denasalesProductCatalog:Staged->Online ;                  ;                  ;  
                                    
#Insert Content Catalogs Synchronization
INSERT_UPDATE CatalogVersionSyncJob ; code[unique=true]                           ; $sourceContentCV ; $targetContentCV ; $contentCatalogsRootTypes 
                                    ; sync denasalesContentCatalog:Staged->Online ;                  ;                  ;                           
