			<attributes>
				<attribute qualifier="code" type="java.lang.String">
					<modifiers initial="true" unique="true" optional="false" />
					<persistence type="property" />
				</attribute>
				<attribute qualifier="catalogVersion" type="CatalogVersion">
					<persistence type="property" />
					<modifiers optional="false" unique="true" />
				</attribute>
				<attribute qualifier="productsSet" type="setOfProducts">
					<persistence type="property" />
				</attribute>
				<attribute qualifier="variantProductsSet" type="setOfProductVariants">
					<persistence type="property" />
				</attribute>
			</attributes>
		</itemtype>
		<itemtype code="DenasalesProduct" jaloclass="com.denasales.core.jalo.DenasalesProduct"
