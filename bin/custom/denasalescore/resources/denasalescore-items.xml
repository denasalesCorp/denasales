		<itemtype code="CMSFooterColumnsParagraphComponent" generate="true"
			jaloclass="com.denasales.core.jalo.CMSInternationalizationParagraphComponent"
			extends="CMSParagraphComponent" autocreate="true">
			<description>It represents columns in footer.</description>
			<attributes>
				<attribute type="footerColumns" qualifier="columns">
					<persistence type="property" />
					<modifiers />
					<description>Map of columns</description>
				</attribute>
				<attribute type="java.lang.String" qualifier="newColumnName">
					<persistence type="property" />
					<modifiers />
					<description>New column will be taken by an interceptor and reset. It will be saved as null in the database the all time.</description>
				</attribute>
				<attribute type="java.lang.String" qualifier="newValueName">
					<persistence type="property" />
					<modifiers />
					<description>A value will be mapped with newColumnName and saved into footerColumns</description>
				</attribute>
				<attribute type="java.lang.String" qualifier="newReferenceForColumnValue">
					<persistence type="property" />
					<modifiers />
					<description>A reference will be injected into 'a' tag. 'a' tag will wrap newValueName value and it will be saved into footerColumns.</description>
				</attribute>
				<attribute type="boolean" qualifier="remove">
					<persistence type="property" />
					<modifiers />
					<defaultvalue>false</defaultvalue>
					<description>If the value is true, a value will be removed.</description>
				</attribute>
			</attributes>
		</itemtype>
