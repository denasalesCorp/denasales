	</maptypes>

	<relations>
		<relation code="DenasalesProduct2DenasalesVariantRelation" autocreate="true"
			generate="true" localized="false">
			<sourceElement qualifier="baseProduct" type="DenasalesProduct" cardinality="one">
				<modifiers read="true" write="true" search="true" optional="false" initial="true" />
			</sourceElement>
			<targetElement qualifier="denasalesVariants" type="DenasalesVariantProduct" cardinality="many" ordered="false">
				<modifiers read="true" write="true" search="true" optional="true" partof="true" />
				<custom-properties>
					<property name="ordering.attribute">
						<value>"code"</value>
					</property>
				</custom-properties>
			</targetElement>
		</relation>
	</relations>

